{
    "manifest_version": 2,

    "name": "dark-newtab",
    "description": "A dark, simple-ish new tab page",
    "version": "0.2",

    "permissions": [
        "bookmarks",
        "storage",
        "chrome://favicon/",
        "http://localhost/"
    ],

    "chrome_url_overrides": {
        "newtab": "newtab.html"
    }
}
